<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest Group</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@200;400;900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="dist/css/main.min.css">
    <script src="https://kit.fontawesome.com/bd49c854da.js" crossorigin="anonymous"></script>
</head>

<body>
    <!-- @include 'nav.kit' -->

    <main>
        <section class="contact">
            <div class="wrapper">


                <h2 class="section-heading">Kontakt</h2>
                <div class="contact__items">

                    <form class="contact__form">

                        <label for="name" class="contact__form-label">Podaj imię:</label>
                        <input type="text" class="contact__form-input" id="name">
                        <label for="email" class="contact__form-label">Podaj e-mail:</label>
                        <input type="email" class="contact__form-input" id="email">
                        <label for="msg" class="contact__form-label" id="msg">Treść wiadomości:</label>
                        <textarea name="msg" id="" cols="30" rows="10" class="contact__form-textarea"></textarea>
                        <button class="contact__form-btn">Wyślij <i class="fa-solid fa-envelope"></i></button>
                    </form>
                    <div class="contact__map">

                        <iframe
                            src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d10239.593805595492!2d19.8928489!3d50.0881881!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1678197206190!5m2!1spl!2spl"
                            allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                        <div class="contact__map-text">
                            <h3 class="contact__map-text-title">Forest Group</h3>
                            <p class="contact__map-text-address">ul. lorem 5</p>
                            <p class="contact__map-text-address">00-000 Lorem</p>
                        </div>

                    </div>
                </div>

            </div>
        </section>
    </main>

    <!-- @include 'footer.kit' -->




    <script src="dist/js/main.min.js"></script>
</body>

</html>